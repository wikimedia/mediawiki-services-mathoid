{
  "name": "mathoid",
  "version": "0.7.2",
  "content-version": "1.0.0",
  "description": "Render TeX to SVG and MathML using MathJax. Based on svgtex.",
  "main": "./app.js",
  "scripts": {
    "start": "service-runner",
    "test": "npm run lint && PREQ_CONNECT_TIMEOUT=15 mocha",
    "lint": "eslint --max-warnings 0 --ext .js --ext .json .",
    "docker-start": "service-runner docker-start",
    "docker-test": "service-runner docker-test",
    "test-build": "service-runner docker-test && service-runner build --deploy-repo --force",
    "coverage": "istanbul cover _mocha -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wikimedia/mathoid.git"
  },
  "keywords": [
    "REST",
    "API",
    "service template",
    "MediaWiki",
    "MathML",
    "SVG",
    "LaTeX",
    "TeX",
    "texvc",
    "math",
    "rendering"
  ],
  "author": "Wikimedia Service Team <services@wikimedia.org>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/mathoid/"
  },
  "homepage": "https://github.com/wikimedia/mathoid",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "cassandra-uuid": "^0.1.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "http-shutdown": "^1.2.0",
    "js-yaml": "^3.12.1",
    "mathoid-mathjax-node": "^0.7.0",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "preq": "^0.5.4",
    "service-runner": "^2.6.9",
    "speech-rule-engine": "^2.1.1",
    "svgo": "^1.0.4",
    "swagger-router": "^0.7.1",
    "texvcinfo": "^0.5.3"
  },
  "optionalDependencies": {
    "librsvg": "^0.7.0",
    "swagger-ui": "git+https://github.com/wikimedia/swagger-ui#master"
  },
  "devDependencies": {
    "ajv": "^6.6.2",
    "commander": "^2.9.0",
    "coveralls": "^3.0.2",
    "dom-compare": "^0.3.1",
    "eslint-config-wikimedia": "^0.10.1",
    "eslint-plugin-jsdoc": "^4.1.0",
    "eslint-plugin-json": "^1.2.0",
    "extend": "^3.0.0",
    "istanbul": "^0.4.5",
    "microtime": "^3.0.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-require": "^3.0.3",
    "rewire": "^4.0.1",
    "xmldom": "^0.1.21"
  },
  "engines": {
    "node": ">=4"
  },
  "deploy": {
    "node": "6.11.1",
    "target": "debian",
    "dependencies": {
      "_all": [
        "librsvg2-dev"
      ]
    }
  }
}
